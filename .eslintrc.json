{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "linebreak-style": 0,
    "max-len": 0,
    "no-console": 0,
    "import/extensions": 0,
    "camelcase": 0,
    "import/prefer-default-export": 0,
    "no-empty-function": 0,
    "no-useless-constructor": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-undef": 0
  }
}
